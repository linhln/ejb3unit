<persistence>
	<persistence-unit name="uns">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<jta-data-source>java:/PostgresqlDS</jta-data-source>
		<properties>
			<property name="hibernate.hbm2ddl.auto" value="create-drop"/>
		</properties>
	</persistence-unit>
</persistence>